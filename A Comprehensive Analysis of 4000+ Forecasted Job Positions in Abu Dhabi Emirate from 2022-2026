    "gender_loc=df.pivot_table(index='Location',columns='Gender',values='Total',aggfunc='sum').fillna(0).astype(int)\n",
    "print(gender_loc)\n",
    "plt.figure(figsize=(10, 4))\n",
    "x = np.arange(3)\n",
    "width= 0.2\n",
    "colors=['darkkhaki','deepskyblue','thistle']\n",
    "plt.bar(x-width,gender_loc['Both'],label='Both',width=width,color=colors[0])\n",
    "plt.bar(x,gender_loc['Male'],label='Male',width=width,color=colors[1])\n",
    "plt.bar(x+width,gender_loc['Female'],label='Female',width=width,color=colors[2])\n",
    "plt.xticks(x, gender_loc.index,fontsize=12)  \n",
    "plt.yticks(fontsize=12)\n",
    "plt.legend()\n",
    "plt.show()"
   ]
