    "edutype_experience = df_repeated.groupby(['Education Group','Education Type'])['Minimum Experience'].mean().to_frame().round(2).sort_values(by='Education Group')\n",
    "edutype_experience['Mean of Minimum Experience in Months']= (edutype_experience['Minimum Experience']*12).round(0).astype(int)\n",
    "edutype_experience.rename(columns={'Minimum Experience':'Mean of Minimum Experience in Years' }, inplace=True)\n",
    "edutype_experience"
