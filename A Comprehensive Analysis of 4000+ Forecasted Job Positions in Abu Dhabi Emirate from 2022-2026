    "edugroup_experience = df_repeated.groupby(['Education Group'])['Minimum Experience'].mean().to_frame().round(2).sort_values(by='Minimum Experience',ascending=False)\n",
    "edugroup_experience['Mean of Minimum Experience in Months'] = (edugroup_experience['Minimum Experience']*12).round(0).astype(int)\n",
    "edugroup_experience.rename(columns={'Minimum Experience':'Mean of Minimum Experience in Years'},inplace=True)\n",
    "edugroup_experience"
   ]
