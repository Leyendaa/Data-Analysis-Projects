    "ESCO_skills = df.groupby(['Equivalent ESCO Occupation title', 'List of ESCO skills based on occupation title'])['Total'].sum().astype(int).reset_index()\n",
    "ESCO_skills = ESCO_skills.sort_values(['Equivalent ESCO Occupation title', 'Total'], ascending=[True, False])\n",
    "ESCO_skills = ESCO_skills.groupby('Equivalent ESCO Occupation title').head(1)\n",
